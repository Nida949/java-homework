import java.util.*;
class hwpg201q11
{
    public static void main()
    {
        int a=0;
        System.out.println("the odd numbers are");
        while(a<10)
        {
            a++;
            if(a%2==0)
            {
                continue;
            }
            System.out.println(a);
        }
    }
}
