import java.util.*;
class hwpg199q10
{
    public static void main ()
    {
        Scanner sc = new Scanner(System.in);
        int i,a,b,c;
        for(i=1;i<=10;i++)
        {
            System.out.println("enter any two numbers");
            a=sc.nextInt();
            b=sc.nextInt();
            c=a+b;
            if(c<0)
            {
                continue;
            }
            System.out.println("the sum of two numbers is" +c);
        }
    }
}
